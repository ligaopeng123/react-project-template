    "scripts": {
        "start": "dotenv -v PORT=9000 -v REACT_APP_SENTRY=false -e .env.dev craco start",
        "start:test": "dotenv -v PORT=9001 -v REACT_APP_SENTRY=false -e .env.test craco start",
        "start:pod": "dotenv -v PORT=9002 -v REACT_APP_SENTRY=false -e .env.pod craco start",
        "build:dev": "set NODE_OPTIONS=--max_old_space_size=36000 && dotenv -e .env.dev craco build && npx rimraf ./build/static/js/*.map",
        "build:test": "set NODE_OPTIONS=--max_old_space_size=36000 && dotenv -e .env.test craco build && npx rimraf ./build/static/js/*.map",
        "build": "set NODE_OPTIONS=--max_old_space_size=36000 && dotenv -e .env.pod craco build && npx rimraf ./build/static/js/*.map ",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "resolutions": {
        "@sentry/cli": "1.74.6"
    },
    "pnpm": {
        "overrides": {
            "@sentry/cli": "1.74.6"
        }
    },
    "overrides": {
        "@sentry/cli": "1.74.6"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "@typescript-eslint/no-unused-vars": "off",
            "react-hooks/exhaustive-deps": "off",
            "no-useless-rename": "off",
            "no-restricted-globals": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@craco/craco": "^6.4.3",
        "@sentry/cli": "^2.10.0",
        "@sentry/webpack-plugin": "^1.20.0",
        "@types/jest": "^27.4.0",
